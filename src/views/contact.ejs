<body>
  
        <nav class="navbar navbar-expand-lg navbar-light fixed-top">
            <div class="container">
    
                <a class="navbar-brand" href="#">THE LIB</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                  <span class="navbar-toggler-icon"></span>
                </button>
              
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                  <ul class="navbar-nav ml-auto">
                    <%for(i=0;i<nav.length;i++){%>
                  <li class="nav-item">
                    <a class="nav-link" href="<%=nav[i].link%>"><%=nav[i].name%><span class="sr-only">(current)</span></a>
                  </li>
                  <%}%>
              </ul>
                </div>
            </div>
              </nav>
   
              <div class=" header">
                <h1 class="text-white text-center mt-auto pt-5" style="font-size:100px;">Contact Us</h1>
             </div>
             
             <div class="container panel">
                 <div class="col-md-10 ml-auto mr-auto text-center">
                  
                  
<!----------------- contact form----------->

<div class="row">

  <div class="col-lg-6 col-md-8 col-sm-12 ml-auto mr-auto">

      <div class="form-row">
                  <!-- <div class="col-md-8"> -->
                    <form name="form1" id="formcntct">
                        <div class="form-group">
                          <label for="exampleFormControlInput1">Enter name:</label>
                          <input type="text" class="form-control" id="exampleFormControlInput1" placeholder="*" required>
                        </div>
                        <div class="form-group">
                            <label for="exampleFormControlInput1">Enter Email ID:</label>
                            <input type="email" class="form-control" id="exampleFormControlInput1" placeholder="*" required>
                        </div>
                        <div class="form-group">
                            <label for="exampleFormControlInput1">Enter phone no:</label>
                            <input type="text" class="form-control" id="exampleFormControlInput1">
                        </div>
                        <div class="form-group">
                          <label for="exampleFormControlTextarea1">Enter feedback:</label>
                          <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" placeholder="*" required></textarea>
                        </div><br>
                        
                        <div class="form-group">
                        <button class="btn btn-block" id="submit">Submit feedback</button>
                        </div>
                   </form>
                </div>

            </div>
</div>

               </div>
             </div>
           
          
              
